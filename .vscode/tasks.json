{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "build",
		"command": "dotnet",
		"type": "process",
		"args": [
		  "build",
		  "${workspaceFolder}/MPCalcHub.csproj"
		],
		"problemMatcher": "$msCompile",
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"detail": "Compila o projeto"
	  },
	  {
		"label": "run",
		"command": "dotnet",
		"type": "process",
		"args": [
		  "run",
		  "--project",
		  "${workspaceFolder}/MPCalcHub.csproj"
		],
		"problemMatcher": [],
		"group": {
		  "kind": "test",
		  "isDefault": true
		},
		"detail": "Executa o projeto"
	  }
	]
  }
  